{
  "name": "@omicronenergy/oscd-scl-dialogs",
  "description": "Provides a resuable dialog for adding and editing SCL elements in OpenSCD.",
  "displayName": "OpenSCD Edit Dialog",
  "version": "0.0.9",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OMICRONEnergyOSS/oscd-scl-dialogs.git"
  },
  "keywords": [
    "scl",
    "xml",
    "iec",
    "61850",
    "iec61850",
    "scd",
    "omicronenergy"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OMICRONEnergyOSS/oscd-scl-dialogs/issues"
  },
  "homepage": "https://github.com/OMICRONEnergyOSS/oscd-scl-dialogs#readme",
  "type": "module",
  "exports": {
    ".": "./dist/foundation.js",
    "./OscdSclDialogs.js": "./dist/OscdSclDialogs.js",
    "./oscd-scl-dialogs.js": "./dist/oscd-scl-dialogs.js",
    "./OscdTextEditor.js": "./dist/OscdTextEditor.js",
    "./oscd-scl-dialogs-events.js": "./dist/oscd-scl-dialogs-events.js",
    "./wizards.js": "./dist/wizards/wizards.js",
    "./patterns.js": "./dist/wizards/patterns.js"
  },
  "files": [
    "dist",
    "custom-elements.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "analyze": "cem analyze",
    "start": "rimraf dist && tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds --watch --node-resolve\"",
    "start:bundle": "npm run bundle && concurrently -k -r \"rollup -c rollup.config.js --watch\" \"wds --watch --root-dir ./dist\"",
    "build": "rimraf dist && tsc && npm run analyze -- --exclude dist",
    "build:watch": "rimraf dist && npm run analyze -- --exclude dist && tsc --watch --preserveWatchOutput",
    "bundle": "rimraf dist && rollup -c rollup.config.js",
    "update-readme": "node dist/scripts/update-readme.js",
    "test": "npm run build && wtr --coverage",
    "test:unit": "npm run build && wtr --coverage --group unit",
    "test:visual": "npm run build && wtr --group visual",
    "test:watch": "npm run build && concurrently -k -r \"tsc -b --watch --preserveWatchOutput\" \"wtr --watch --coverage\"",
    "test:unit:watch": "npm run build && concurrently -k -r \"tsc -b --watch --preserveWatchOutput\" \"wtr --watch --coverage  --group unit\"",
    "test:update": "npm run build && wtr --group visual --update-visual-baseline",
    "prepare": "husky"
  },
  "dependencies": {
    "@omicronenergy/oscd-editor-source": "^0.0.3",
    "@omicronenergy/oscd-shell": "^0.0.11",
    "@open-wc/scoped-elements": "^3.0.6",
    "@openenergytools/filterable-lists": "^1.2.0",
    "@openenergytools/scl-checkbox": "^2.0.0",
    "@openenergytools/scl-select": "^2.0.1",
    "@openenergytools/scl-text-field": "^2.0.2",
    "@openscd/oscd-api": "^0.1.6",
    "@openscd/oscd-editor": "^1.8.0",
    "@openscd/scl-lib": "^2.0.0",
    "@scopedelement/material-web": "^3.12.0",
    "@webcomponents/scoped-custom-element-registry": "^0.0.10",
    "ace-builds": "^1.43.6",
    "ace-custom-element": "^1.6.5",
    "lit": "^3.3.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@custom-elements-manifest/analyzer": "^0.11.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/dev-server-hmr": "^0.2.0",
    "@open-wc/eslint-config": "^13.0.0",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@web/dev-server": "^0.4.6",
    "@web/dev-server-esbuild": "^1.0.5",
    "@web/dev-server-polyfill": "^1.0.6",
    "@web/dev-server-storybook": "^2.0.3",
    "@web/rollup-plugin-import-meta-assets": "^2.3.2",
    "@web/test-runner": "0.20.2",
    "@web/test-runner-commands": "0.9.0",
    "@web/test-runner-playwright": "0.11.1",
    "@web/test-runner-visual-regression": "0.10.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-lit-a11y": "^5.1.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-tsdoc": "^0.5.0",
    "eslint-plugin-wc": "^3.0.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "rollup-plugin-copy": "^3.5.0",
    "sinon": "^21.0.1",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "playwright": "1.54.1"
  },
  "customElements": "custom-elements.json",
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "eslint"
    ]
  }
}
